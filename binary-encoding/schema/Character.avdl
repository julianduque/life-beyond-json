protocol Schema {
  record Attribute {
    int strength;
    int dexterity;
    int constitution;
    int intelligence;
    int wisdom;
    int charisma;
  }

  record Character {
    string name;
    string player;
    string profession;
    string species;
    boolean playing = true;
    int level;
    Attribute attributes;
    array<string> proficiencies;
  }
}